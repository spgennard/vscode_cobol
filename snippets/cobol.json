	">>define directive": {
		"prefix": ">>define",
		"body": [
			">>define ${1:variable-name} as ${2|10,41 + 1,parameter|}",
			"$0"
		],
		"description": ">>define directive",
		"scope": "cobol"
	},
	">>define directive + override": {
		"prefix": ">>define",
		"body": [
			">>define ${1:variable-name} as ${2|10,41 + 1,parameter|} ${3:override}",
