    "_copyright": "The MIT License (MIT)\nCopyright (c) 2015-2022 spgennard\nSource: https://github.com/spgennard/vscode_cobol/blob/main/syntaxes/bmsmap.tmLanguage.json",
    "$schema": "https://raw.githubusercontent.com/spgennard/vscode_cobol/main/schemas/tmlanguage.json",
    "fileTypes": [],
    "name": "bmsmap",
    "patterns": [
        {
            "match": "(\\f)",
            "name": "strong comment.line.form_feed.bmsmap"
        },
        {
            "match": "(^\\*.*$)",
            "name": "comment.line.modern"
        },
        {
            "begin": "^(=COLS>)\\s",
            "name": "constant.numeric.bmsmap",
            "patterns": [
                {
                    "match": "(.*)",
                    "name": "markup.heading"
                }
            ],
            "end": "($)"
        },
        {
            "begin": "^\\s*([0-9][0-9][0-9][0-9][0-9][0-9])\\s",
            "beginCaptures": {
                "0": {
                    "name": "constant.numeric.bmsmap"
                }
            },
            "patterns": [
                {
                    "include": "source.bms"
                }
            ],

            "end": "($)"
        },
        {
            "match": "(.*$)"
        }
    ],
    "scopeName": "source.bmsmap"
}